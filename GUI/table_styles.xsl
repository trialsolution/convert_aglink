        border-top-style:solid;
        border-top-width:5px;

        border-bottom-style:solid;
        border-bottom-width:5px;
     }

    /* the cells in the second line of the table comprising the name of the current innercolumn element */
    .innercolumnname
     {
        text-align:center;
        border-bottom-style:solid;
        border-bottom-width:5px;
     }

    /* the first of the cells in the second line of the table comprising the name of the current innercolumn element */
    .firstinnercol
     {
        text-align:center;
        border-left-style:solid;
        border-left-width:5px;

        border-bottom-style:solid;
        border-bottom-width:5px;
     }

    /* for units in the description cells */
    .unit
    {
        font-size:7pt;
        color:black;
        vertical-align:bottom;
    }

    /* for the precentage changes against the compare scenario */
    .percent
    {
        font-size:7pt;
    }

    /* the cell with the numbers in the table */
    .cell
     {
        background-color:#F0F8FF;
        color:black;
        font-weight:normal;
        min-height:20px;
        max-height:20px;
     }

    /* the cell with the numbers in the table */
    .left
     {
        background-color:#F0F8FF;
        color:black;
        font-weight:normal;
        text-align:left;
     }

    /* the cell with the numbers in the table */
    .center
     {
        background-color:#F0F8FF;
        color:black;
        font-weight:bold;
        text-align:center;
     }

    /* the first cell in each column with the numbers in the table (to introduce thicker border) */
    .firstcell
     {
        background-color:#F0F8FF;
        color:black;
        font-weight:normal;
        border-left-style:solid;
        border-left-width:5px;
     }

     /* upper left corner of the table */
    .corner
     {
        text-align:left;
        color:black;

        border-top-style:solid;
        border-top-width:5px;

        border-bottom-style:solid;
        border-bottom-width:5px;
     }

    .left
     {
        text-align:left;
     }


     /* properties of the table style plus properties for the cells if not set otherwisel  */
    .table
     {

        padding:0px;

        border-width:5px;
        border-style:solid;
        border-collapse: collapse;
        border-spacing:  collapse;

        background-color:#DCEBFA;
        color:#000066;
        font-weight:bold;
        font-size:8pt;
        font-family:Arial;
        text-align:right;


