    /* the cell at the left border of the table comprising the name of the current row element */
    .rowname
     {
        // text-align:left;
        vertical-align:top;
        min-height:20px;
        max-height:20px;
     }

    /* the second cell from the left border of the table comprising the name of the current innerrow element */
    .innerrowname
     {
